{
  "manifest_version": 3,
  "name": "TwitchTweaks",
  "version": "1.1",
  "description": "TwitchTweaks - Better Twitch!",
  "permissions": ["clipboardWrite"],
  "content_scripts": [
    {
      "matches": ["https://*.twitch.tv/*"],
      "js": [
        "m/upload/upload.js",
        "m/panel/osu/map.js",
        "m/chat/showfiles/images.js",
        "m/chat/showfiles/text.js"
      ]
    }
  ]
}